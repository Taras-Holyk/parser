{
    "extends": "standard",
    "env": {
        "node": true
    },
    "rules": {
        "no-trailing-spaces": ["error", { "skipBlankLines": true }],
        "space-before-function-paren": ["error", {
            "anonymous": "always",
            "named": "never",
            "asyncArrow": "always"
        }],
        "semi": [2, "always"]
    }
}
